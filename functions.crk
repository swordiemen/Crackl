Program {

	func int mult(int valA, int valB){
		int result = valA;
		while(valB>1){
			result = result + valA;
			valB = valB - 1;
		}
		print(result);
		return result;
	}
	
	//print n multiples of mult//
	func int multiples(int n, int mult){
		int i = 0;
		while(i < (n+1)){
			mult(mult, i);
			i = i+1;
		}
		return 9;
	}

	
	main {
		//Expected result: 6, 8 and 9//
		mult(2,3);
		mult(2,4);
		mult(3,3);
		//multiples(3,3); broken :(//
	}
	
}