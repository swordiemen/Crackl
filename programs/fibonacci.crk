Program {

	func int fib(int n)
	{
		int result = 0;
		if(n < 2)
		{
			result = n;
		}
		else{
			result = fib(n-1);
			result =  fib(n-2) + result; // only head recursion...//
		}
		return result;
	}

	main {
		int number = 0;
		while(number < 6+1){
			print(fib(number));
			number = number + 1;
		}
	}
	
}